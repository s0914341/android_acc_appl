<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/laySwipe"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"> 
        
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF111111"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true" >

    <RelativeLayout  
        	android:layout_height="250dp" 
        	android:id="@+id/frameLayout2" 
        	android:layout_width="match_parent" 
        	android:background="@drawable/border2">
        	
        	<TextView 
            	android:id="@+id/MachineInformation" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="Machine status info"
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="10dp"
            	android:textStyle="bold">
            </TextView>
 
        	<TextView 
            	android:id="@+id/ViewMassStorage" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="MassStorage"
            	android:layout_marginLeft="42dp"
            	android:layout_marginTop="50dp">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewShaker" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Shaker"
            	android:layout_toRightOf="@+id/ViewMassStorage" 
            	android:layout_alignTop="@+id/ViewMassStorage" 
            	android:layout_alignBottom="@+id/ViewMassStorage"
            	android:layout_alignLeft="@+id/ShakerStatus">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewSensor" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Sensor"
            	android:layout_toRightOf="@+id/ViewShaker" 
            	android:layout_alignTop="@+id/ViewShaker" 
            	android:layout_alignBottom="@+id/ViewShaker"
            	android:layout_alignLeft="@+id/SensorStatus">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewConnect" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Connect"
            	android:layout_toRightOf="@+id/ViewSensor" 
            	android:layout_alignTop="@+id/ViewSensor" 
            	android:layout_alignBottom="@+id/ViewSensor"
            	android:layout_alignLeft="@+id/ConnectStatus">
            </TextView>
             
            <ImageView 
            	android:layout_width="100dp" 
            	android:src="@drawable/view_mass_storage" 
            	android:id="@+id/MassStorageStatus" 
            	android:layout_height="100dp" 
                android:layout_marginTop="100dp"
            	android:layout_marginLeft="42dp">
            </ImageView>
            
            <ImageView 
            	android:layout_width="100dp" 
            	android:src="@drawable/view_shaker" 
            	android:id="@+id/ShakerStatus" 
            	android:layout_height="100dp" 
            	android:layout_marginLeft="80dp"
            	android:layout_toRightOf="@+id/MassStorageStatus" 
            	android:layout_alignTop="@+id/MassStorageStatus" 
            	android:layout_alignBottom="@+id/MassStorageStatus">
            </ImageView>
            
             <ImageView 
            	android:layout_width="100dp" 
            	android:src="@drawable/view_sensor" 
            	android:id="@+id/SensorStatus" 
            	android:layout_height="100dp" 
            	android:layout_marginLeft="80dp"
            	android:layout_toRightOf="@+id/ShakerStatus" 
            	android:layout_alignTop="@+id/ShakerStatus" 
            	android:layout_alignBottom="@+id/ShakerStatus">
            </ImageView>
             
             <ImageView 
            	android:layout_width="100dp" 
            	android:src="@drawable/view_connect" 
            	android:id="@+id/ConnectStatus" 
            	android:layout_height="100dp" 
            	android:layout_marginLeft="80dp"
            	android:layout_toRightOf="@+id/SensorStatus" 
            	android:layout_alignTop="@+id/SensorStatus" 
            	android:layout_alignBottom="@+id/SensorStatus">
            </ImageView>
    </RelativeLayout>
    
    <RelativeLayout 
        	android:layout_height="160dp" 
        	android:id="@+id/relativeLayout1" 
        	android:layout_width="fill_parent" 
        	android:background="@drawable/border2">
        	
        	<TextView 
            	android:id="@+id/textViewButton1" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="Start"
            	android:layout_marginLeft="55dp"
            	android:layout_marginTop="20dp"
            	android:textStyle="bold">
            </TextView>
            
        	<!-- <TextView 
            	android:id="@+id/textViewButton2" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_toRightOf="@+id/textViewButton1" 
            	android:layout_alignTop="@+id/textViewButton1" 
            	android:layout_alignLeft="@+id/Button2"
            	android:layout_alignBottom="@+id/textViewButton1"
            	android:text="Get Data"
            	android:textStyle="bold">
            </TextView> -->
            
        	<TextView 
            	android:id="@+id/textViewButton3" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_toRightOf="@+id/textViewButton1" 
            	android:layout_alignLeft="@+id/Button3"
            	android:layout_alignTop="@+id/textViewButton1" 
            	android:layout_alignBottom="@+id/textViewButton1"
            	android:text="Send Script"
            	android:textStyle="bold">
            </TextView>
            
        	<TextView 
            	android:id="@+id/textViewButton4" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_toRightOf="@+id/textViewButton3" 
            	android:layout_alignLeft="@+id/Button4"
            	android:layout_alignTop="@+id/textViewButton3" 
            	android:layout_alignBottom="@+id/textViewButton3"
            	android:text="Chart"
            	android:textStyle="bold">
            </TextView>
            
        	<TextView 
            	android:id="@+id/textViewButton5" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_toRightOf="@+id/textViewButton4" 
            	android:layout_alignLeft="@+id/Button5"
            	android:layout_alignTop="@+id/textViewButton4" 
            	android:layout_alignBottom="@+id/textViewButton4"
            	android:text="Script Set"
            	android:textStyle="bold">
            </TextView>
        	
            <ToggleButton 
                android:textOn="" 
                android:textOff=""
                android:background="@drawable/start_button" 
            	android:scaleType="fitCenter" 
            	android:layout_width="80dp" 
            	android:layout_height="80dp" 
            	android:id="@+id/Button1" 
            	android:adjustViewBounds="false" 
            	android:layout_marginTop="50dp"
            	android:layout_marginLeft="42dp" android:focusable="true">
            </ToggleButton>
            
          <!--   <ImageButton
           	 		android:background="@drawable/buttons_get_data" 
           	 		android:id="@+id/Button2" 
           	 		android:layout_height="80dp" 
           	 		android:layout_width="80dp"
           	 		android:scaleType="fitCenter" 
            		android:adjustViewBounds="false"
            		android:layout_marginLeft="30dp"   	 		
           	 		android:layout_toRightOf="@+id/Button1" 
           	 		android:layout_alignTop="@+id/Button1">
           	</ImageButton> -->
             
           	<ImageButton 
            		android:background="@drawable/buttons_send_script" 
            		android:layout_width="80dp" 
            		android:id="@+id/Button3" 
            		android:layout_height="80dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button1"            		
            		android:layout_alignTop="@+id/Button1"
            		android:scaleType="fitCenter" 
            		android:layout_marginLeft="30dp">
            </ImageButton>           
               
           	<ImageButton 
            		android:background="@drawable/buttons_chart" 
            		android:layout_width="80dp" 
            		android:id="@+id/Button4" 
            		android:layout_height="80dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button3"            		
            		android:layout_alignTop="@+id/Button3"
            		android:scaleType="fitCenter" 
            		android:layout_marginLeft="30dp">
            </ImageButton> 
            
           	<ImageButton 
            		android:background="@drawable/buttons_script" 
            		android:layout_width="80dp" 
            		android:id="@+id/Button5" 
            		android:layout_height="80dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button4"            		
            		android:layout_alignTop="@+id/Button4"
            		android:scaleType="fitCenter"
            		android:layout_marginLeft="30dp">
            </ImageButton> 
            
           	<!-- <ImageButton 
            		android:background="@drawable/buttons_offline" 
            		android:layout_width="80dp" 
            		android:id="@+id/Button6" 
            		android:layout_height="80dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button5"            		
            		android:layout_alignTop="@+id/Button5"
            		android:scaleType="fitCenter"
            		android:layout_marginLeft="30dp">
            </ImageButton>  -->
            
        </RelativeLayout>          
        
        <RelativeLayout  
        	android:layout_height="400dp" 
        	android:id="@+id/frameLayoutShaker" 
        	android:layout_width="match_parent" 
        	android:background="@drawable/border2">
        	<TextView 
            	android:id="@+id/shaker" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="Shaker Command"
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="10dp"
            	android:textStyle="bold">
            </TextView>
            
        	<EditText
        	    android:id="@+id/etInput"
        	    android:layout_width="270dp"
        	    android:layout_height="80dp"
        	    android:layout_marginLeft="25dp"
        	    android:layout_marginTop="40dp" >
            </EditText>
            
        	<TextView 
            	android:id="@+id/ShakerReturn" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="80dp"
            	android:layout_alignParentLeft="true" 
            	android:text="showing..."
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="140dp"
            	android:textStyle="bold">
            </TextView>
            
        	<TextView 
            	android:id="@+id/DebugView" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="150dp"
            	android:layout_alignParentLeft="true" 
            	android:text="debug..."
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="220dp"
            	android:textStyle="bold">
            </TextView>
                                 
        </RelativeLayout>
        
</LinearLayout> 
 </ScrollView>  
</android.support.v4.widget.SwipeRefreshLayout>