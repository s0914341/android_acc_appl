        <RelativeLayout  
        	android:layout_height="150dp" 
        	android:id="@+id/frameLayout2" 
        	android:layout_width="match_parent" 
        	android:background="@drawable/border2">
        	<TextView 
            	android:id="@+id/MachineInformation" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="Machine status info"
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="10dp"
            	android:textStyle="bold">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewMassStorage" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="MassStor"
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="50dp">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewShaker" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Shaker"
            	android:layout_toRightOf="@+id/ViewMassStorage" 
            	android:layout_alignTop="@+id/ViewMassStorage" 
            	android:layout_alignBottom="@+id/ViewMassStorage"
            	android:layout_marginLeft="30dp">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewSensor" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Sensor"
            	android:layout_toRightOf="@+id/ViewShaker" 
            	android:layout_alignTop="@+id/ViewShaker" 
            	android:layout_alignBottom="@+id/ViewShaker"
            	android:layout_marginLeft="30dp">
            </TextView>
            
        	<TextView 
            	android:id="@+id/ViewConnect" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:text="Connect"
            	android:layout_toRightOf="@+id/ViewSensor" 
            	android:layout_alignTop="@+id/ViewSensor" 
            	android:layout_alignBottom="@+id/ViewSensor"
            	android:layout_marginLeft="30dp">
            </TextView>
             
            <ImageView 
            	android:layout_width="50dp" 
            	android:src="@drawable/image0" 
            	android:id="@+id/MassStorageStatus" 
            	android:layout_height="50dp" 
            	android:layout_marginTop="80dp"
            	android:layout_marginLeft="42dp">
            </ImageView>
            
            <ImageView 
            	android:layout_width="50dp" 
            	android:src="@drawable/image0" 
            	android:id="@+id/ShakerStatus" 
            	android:layout_height="50dp" 
            	android:layout_marginLeft="42dp"
            	android:layout_toRightOf="@+id/MassStorageStatus" 
            	android:layout_alignTop="@+id/MassStorageStatus" 
            	android:layout_alignBottom="@+id/MassStorageStatus">
            </ImageView>
            
             <ImageView 
            	android:layout_width="50dp" 
            	android:src="@drawable/image0" 
            	android:id="@+id/SensorStatus" 
            	android:layout_height="50dp" 
            	android:layout_marginLeft="42dp"
            	android:layout_toRightOf="@+id/ShakerStatus" 
            	android:layout_alignTop="@+id/ShakerStatus" 
            	android:layout_alignBottom="@+id/ShakerStatus">
            </ImageView>
            
              <ImageView 
            	android:layout_width="50dp" 
            	android:src="@drawable/image0" 
            	android:id="@+id/ConnectStatus" 
            	android:layout_height="50dp" 
            	android:layout_marginLeft="42dp"
            	android:layout_toRightOf="@+id/SensorStatus" 
            	android:layout_alignTop="@+id/SensorStatus" 
            	android:layout_alignBottom="@+id/SensorStatus">
            </ImageView>
        </RelativeLayout>
        
        <RelativeLayout  
        	android:layout_height="310dp" 
        	android:id="@+id/frameLayoutShaker" 
        	android:layout_width="match_parent" 
        	android:background="@drawable/border2">
        	<TextView 
            	android:id="@+id/shaker" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="wrap_content" 
            	android:layout_alignParentLeft="true" 
            	android:text="Shaker Command"
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="10dp"
            	android:textStyle="bold">
            </TextView>
            
        	<EditText
        	    android:id="@+id/etInput"
        	    android:layout_width="270dp"
        	    android:layout_height="80dp"
        	    android:layout_marginLeft="25dp"
        	    android:layout_marginTop="40dp" >
            </EditText>
            
        	<TextView 
            	android:id="@+id/ShakerReturn" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="80dp"
            	android:layout_alignParentLeft="true" 
            	android:text="showing..."
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="140dp"
            	android:textStyle="bold">
            </TextView>
            
        	<TextView 
            	android:id="@+id/DebugView" 
            	android:textAppearance="?android:attr/textAppearanceMedium"
            	android:layout_width="wrap_content" 
            	android:layout_height="80dp"
            	android:layout_alignParentLeft="true" 
            	android:text="debug..."
            	android:layout_marginLeft="25dp"
            	android:layout_marginTop="220dp"
            	android:textStyle="bold">
            </TextView>
                                 	
