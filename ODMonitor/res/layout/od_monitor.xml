            	android:layout_marginLeft="62dp">
            </ImageView>
            
           	<ImageButton 
            		android:background="@drawable/buttons_send_script" 
            		android:layout_width="65dp" 
            		android:id="@+id/Button3" 
            		android:layout_height="65dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button2"            		
            		android:layout_alignTop="@+id/Button2"
            		android:scaleType="fitCenter" 
            		android:layout_alignLeft="@+id/LED3">
            </ImageButton>           
            
            <ImageView 
            	android:layout_width="50dp" 
            	android:src="@drawable/image0" 
            	android:id="@+id/LED4" 
            	android:layout_height="50dp" 
            	android:layout_toRightOf="@+id/LED3" 
            	android:layout_alignTop="@+id/LED3" 
            	android:layout_alignBottom="@+id/LED3"
            	android:layout_centerVertical="true"
            	android:layout_marginLeft="62dp">
            </ImageView>
            
           	<ImageButton 
            		android:background="@drawable/buttons_chart" 
            		android:layout_width="65dp" 
            		android:id="@+id/Button4" 
            		android:layout_height="65dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button3"            		
            		android:layout_alignTop="@+id/Button3"
            		android:scaleType="fitCenter" 
            		android:layout_alignLeft="@+id/LED4">
            </ImageButton> 
            
           	<ImageButton 
            		android:background="@drawable/buttons" 
            		android:layout_width="65dp" 
            		android:id="@+id/Button5" 
            		android:layout_height="65dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button4"            		
            		android:layout_alignTop="@+id/Button4"
            		android:scaleType="fitCenter"
            		android:layout_marginLeft="62dp">
            </ImageButton> 
            
           	<ImageButton 
            		android:background="@drawable/buttons" 
            		android:layout_width="65dp" 
            		android:id="@+id/Button6" 
            		android:layout_height="65dp"
            		android:adjustViewBounds="false" 
            		android:layout_toRightOf="@+id/Button5"            		
            		android:layout_alignTop="@+id/Button5"
            		android:scaleType="fitCenter"
            		android:layout_marginLeft="62dp">
            </ImageButton> 
            
        </RelativeLayout>          
